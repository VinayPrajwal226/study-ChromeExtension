{
    "manifest_version": 2,
    "name": "Study Buddy",
    "version": "0.1.1",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png",
        "default_title": "Study Buddy"
    },
    "icons": {
        "128": "icon128.png"
    }, 
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}